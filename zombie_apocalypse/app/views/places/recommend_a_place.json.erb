{
	status: “OK”,
	response: {
		user: {
			user_id: <%= @person.id %>,
			name: <%= @person.name %>,
			location: {
				latitude: <%= @person.latitude %>,
				longitude: <%= @person.longitude %>
			},
			armory: <%= @person.has_weapon %>,
			hungry: <%= @person.is_hungry %>,
			alone: <%= @person.is_alone %>
		},
		places : [
			<% @sorted_array_values_places.each_with_index do |place, index| %>
				{
				place_id: <%= place.id %>,
				name: <%= place.name %>,
				location: {
					latitude: <%= place.latitude %>,
					longitude: <%= place.longitude %>
				},
				armory: <%= place.has_weapon %>,
				food: <%= place.has_food %>,
				people: <%= place.has_people %>
				<% if index < (@sorted_array_values_places.length - 1) %>
					},
				<% else %>
					}
				<% end %>

			<% end %>
		]
}