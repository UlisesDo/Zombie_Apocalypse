{
	status: “OK”,
	response: {
		user: {
			user_id: <%= @person.id %>,
			name: <%= @person.name %>,
			location: {
				latitude: <%= @person.latitude %>,
				longitude: <%= @person.longitude %>
			},
			armory: <%= @person.has_weapon %>,
			hungry: <%= @person.is_hungry %>,
			alone: <%= @person.is_alone %>
		},
		places : [
			<% @sorted_array_values_places.each_with_index do |value_place_array, index| %>
				{
				place_id: <%= value_place_array[1].id %>,
				name: <%= value_place_array[1].name %>,
				location: {
					latitude: <%= value_place_array[1].latitude %>,
					longitude: <%= value_place_array[1].longitude %>
				},
				armory: <%= value_place_array[1].has_weapon %>,
				food: <%= value_place_array[1].has_food %>,
				people: <%= value_place_array[1].has_people %>
				<% if index < (@sorted_array_values_places.length - 1) %>
					},
				<% else %>
					}
				<% end %>

			<% end %>
		]
}